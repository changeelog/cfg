{
  "Basic EditorConfig": {
    "prefix": "basic",
    "body": [
      "root = true",
      "",
      "[*]",
      "charset = utf-8",
      "end_of_line = lf",
      "insert_final_newline = true",
      "trim_trailing_whitespace = true",
      "indent_style = space",
      "indent_size = 2"
    ],
    "description": "Basic EditorConfig setup"
  },
  "EditorConfig for Web Projects": {
    "prefix": "web",
    "body": [
      "root = true",
      "",
      "[*]",
      "charset = utf-8",
      "end_of_line = lf",
      "insert_final_newline = true",
      "trim_trailing_whitespace = true",
      "indent_style = space",
      "indent_size = 2",
      "",
      "[*.{js,jsx,ts,tsx,vue}]",
      "indent_size = 2",
      "",
      "[*.{css,scss,less,sass}]",
      "indent_size = 2",
      "",
      "[*.html]",
      "indent_size = 2",
      "",
      "[*.md]",
      "trim_trailing_whitespace = false",
      "",
      "[*.json]",
      "indent_size = 2"
    ],
    "description": "EditorConfig setup for web projects"
  }
}
