  "LESS Color Function": {
    "prefix": "lcolor",
    "description": "Color Function",
    "body": ["${1:color-function}(${2:color}, ${3:adjustment});"]
  },
  "LESS Each Loop": {
    "prefix": "leach",
    "description": "Each Loop",
    "body": [".each(${1:item} in ${2:list}) {", "  ${3:// styles}", "}"]
  },
  "LESS Extend Selector": {
    "prefix": "lextend",
    "description": "Extend Selector",
    "body": ["${1:selector}:extend(${2:other-selector});"]
  },
  "LESS For Loop": {
    "prefix": "lfor",
    "description": "For Loop",
    "body": [
      ".loop(${1:from}, ${2:to}) when (${1:from} <= ${2:to}) {",
      "  ${3:// styles}",
      "  .loop(${1:from} + 1, ${2:to});",
      "}"
    ]
  },
  "LESS Forward Module": {
    "prefix": "lforward",
    "description": "Forward Module",
    "body": ["@forward '${1:module}';"]
  },
  "LESS Function Declaration": {
    "prefix": "lfunc",
    "description": "Function Declaration",
    "body": [
      ".${1:function-name}(${2:arguments}) {",
      "  @return ${3:value};",
      "}"
    ]
  },
  "LESS If Statement": {
    "prefix": "lif",
    "description": "If Statement",
    "body": ["@if (${1:condition}) {", "  ${2:// styles}", "}"]
  },
  "LESS Import Partial": {
    "prefix": "limp",
    "description": "Import Partial",
    "body": ["@import '${1:partial}';"]
  },
  "LESS Include Mixin": {
    "prefix": "linclude",
    "description": "Include Mixin",
    "body": [".${1:mixin-name}(${2:arguments});"]
  },
  "LESS Map Declaration": {
    "prefix": "lmap",
    "description": "Map Declaration",
    "body": ["@${1:map-name}: {", "  ${2:key}: ${3:value};", "};"]
  },
  "LESS Mixin Declaration": {
    "prefix": "lmixin",
    "description": "Mixin Declaration",
    "body": [".${1:name}(${2:arguments}) {", "  ${3:// styles}", "}"]
  },
  "LESS Nesting Selector": {
    "prefix": "lnest",
    "description": "Nesting Selector",
    "body": [
      "${1:parent-selector} {",
      "  ${2:child-selector} {",
      "    ${3:// styles}",
      "  }",
      "}"
    ]
  },
  "LESS String Interpolation": {
    "prefix": "linterp",
